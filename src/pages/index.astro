---
import Layout from '../layouts/Layout.astro';
import CTA from '../content/CTA.astro'
import CarouselSection from '../content/CarouselSection.astro'
---

<Layout title="MegaGym">
	<main>
		<CTA />
		<CarouselSection />
	</main>
</Layout>

<style>
	main {
		color: var(--false-black);
		padding: .5rem;
		width: 100vw;
		display: flex;
		flex-direction: column;
		row-gap: 1rem;
	}
</style>
